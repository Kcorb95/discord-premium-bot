  "devDependencies": {
    "eslint": "^5.6.1",
    "eslint-config-aqua": "^4.4.1"
  },
  "eslintConfig": {
    "extends": "aqua",
    "rules": {
      "es6": "true",
      "no-process-env": "off",
      "comma-dangle": "off",
      "quote-props": "off",
      "no-use-before-define": "off",
      "max-len": "off",
      "eol-last": "off",
      "no-console": "off",
      "no-inline-comments": "off",
      "capitalized-comments": "off",
      "no-undef": "off",
      "max-depth": "warn",
      "curly": "off",
      "nonblock-statement-body-position": "off",
      "indent": "off",
      "prefer-destructuring": [
        "warn"
      ]
    }
  },
  "engines": {
    "node": ">=8.6.0"
  }
